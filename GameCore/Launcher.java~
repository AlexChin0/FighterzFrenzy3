//This is just the main method. It opens the run window and starts the game.

package GameCore;

public class Launcher {
  
  public static int runWidth = 1216, runHeight = 832;
  
  public static void main(String[] args){ 
    
   Game game = new Game("Fighterz Frenzy 2: Hero's Rising", runWidth, runHeight); //launches the run window.
   game.start();
    
  }
 
}
